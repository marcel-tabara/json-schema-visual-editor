{
  "name": "jsonschema-visual-editor",
  "homepage": "https://marcel-tabara.github.io/json-schema-visual-editor/",
  "version": "1.0.0",
  "description": "jsonschema visual editor",
  "main": "src/app/index.js",
  "scripts": {
    "test": "",
    "client": "parcel src/app/index.html --open --out-dir dist ",
    "server": "nodemon ./server.js",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "parcel build src/app/index.html",
    "predeploy": "parcel build src/app/index.html --out-dir dist --public-url jsonschema-visual-editor",
    "deploy": "gh-pages -d docs"
  },
  "heroku-run-build-script": true,
  "browserslist": [
    "last 1 Chrome version"
  ],
  "keywords": [
    "jsonschema",
    "editor",
    "uischema",
    "json"
  ],
  "author": "Marcel Tabara",
  "license": "ISC",
