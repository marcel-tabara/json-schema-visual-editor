    "@fortawesome/react-fontawesome": "^0.1.4",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "brace": "^0.11.1",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
